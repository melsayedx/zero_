{
  "name": "log-ingestion-platform",
  "version": "1.0.0",
  "description": "Minimal log ingestion platform with clean architecture",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:http2": "node src/app-http2.js",
    "start:http3": "node src/app-http3.js",
    "start:cluster": "node cluster.js",
    "start:cluster:dev": "node cluster.js --workers 4",
    "dev": "nodemon src/app.js",
    "dev:http2": "nodemon src/app-http2.js",
    "dev:http3": "nodemon src/app-http3.js",
    "cluster": "node cluster.js",
    "cluster:dev": "nodemon cluster.js",
    "cluster:prod": "NODE_ENV=production node cluster.js",
    "setup:certs": "bash setup-certs.sh",
    "grpc:example": "node grpc-client-example.js",
    "test:json": "node test-json-backward-compatible.js",
    "test:protobuf": "node test-protobuf-single.js",
    "test:protobuf-batch": "node test-protobuf-batch.js",
    "test:all": "./test-all-formats.sh",
    "test:batch-validation": "node test-batch-validation.js",
    "test:batch-buffer": "node test-batch-buffer.js",
    "test:before-after": "node test-clickhouse-before-after.js",
    "test:http2": "./run-http2.sh",
    "test:http3": "./run-http3.sh",
    "perf:protobuf": "node performance-test-protobuf.js",
    "benchmark:batch": "node benchmark-batch-validation.js"

  },
  "keywords": [
    "logs",
    "ingestion",
    "clickhouse"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clickhouse/client": "^0.2.10",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "protobufjs": "^7.5.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
