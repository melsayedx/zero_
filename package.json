{
  "name": "log-ingestion-platform",
  "version": "1.0.0",
  "description": "Minimal log ingestion platform with clean architecture",
  "main": "src/app.js",
  "scripts": {
    "build:proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:src/infrastructure/grpc/generated --grpc_out=grpc_js:src/infrastructure/grpc/generated proto/**/*.proto",
    "build:proto:ts": "grpc_tools_node_protoc --plugin=protoc-gen-ts=node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:src/infrastructure/grpc/generated proto/**/*.proto",
    "build": "npm run build:proto && node src/app.js",
    "start:http2": "ENABLE_HTTP2=true npm run build",
    "start:cluster": "npm run build:proto && node cluster.js",
    "dev:custom": "npm run build:proto && nodemon --max-old-space-size=2048 --max-semi-space-size=64 src/app.js",
    "dev": "npm run build:proto && nodemon src/app.js",
    "dev:http2": "ENABLE_HTTP2=true npm run dev",
    "cluster": "npm run build:proto && node cluster.js",
    "cluster:dev": "npm run build:proto && nodemon cluster.js",
    "cluster:dev:http2": "ENABLE_HTTP2=true npm run cluster:dev",
    "cluster:prod": "NODE_ENV=production npm run build:proto && NODE_ENV=production node cluster.js",
    "setup:certs": "bash setup-certs.sh",
    "benchmark": "node benchmark/benchmark-runner.js",
    "benchmark:baseline": "node benchmark/stages/01-baseline.js",
    "benchmark:ff": "node benchmark/stages/02-fire-and-forget.js",
    "benchmark:coalesce": "node benchmark/stages/03-coalescing.js",
    "benchmark:pipeline": "node benchmark/stages/04-redis-streams.js",
    "benchmark:workers": "node benchmark/stages/05-worker-threads.js",
    "benchmark:report": "node benchmark/generate-report.js"
  },
  "keywords": [
    "logs",
    "ingestion",
    "clickhouse"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clickhouse/client": "^1.14.0",
    "@dotenvx/dotenvx": "^1.51.2",
    "@fastify/compress": "^8.3.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@grpc/grpc-js": "^1.14.2",
    "@grpc/proto-loader": "^0.8.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "argon2": "^0.44.0",
    "fastify": "^5.6.2",
    "google-protobuf": "^4.0.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.1",
    "nanoid": "^5.1.6",
    "protobufjs": "^7.5.4"
  },
  "devDependencies": {
    "chart.js": "^4.5.1",
    "grpc-tools": "^1.13.1",
    "nodemon": "^3.1.11",
    "perf_hooks": "^0.0.1"
  }
}